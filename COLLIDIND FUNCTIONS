import math
defball_collide(x1,x2,r1,r2,y1,y2)
#calculate the distance between the centers of two balls 
dist = math.sqrt((x2-x1)**2+(y2-y1)**2)
print("distance between two balls:",dist)
# calculate the sum of the radii of the two balls
r=r1+r2
print("sum of the radii ",r)
#check if the distamce between the centers is less than or equal to the sum of the radii
if dist <=r:
print("they are colliding ")
return true
else:
print("not colliding")
return ture 
c1=ball_collide(4,4,3,2,2,3)
print(c1)
c2=ball_collide(100,200,30,300,100,10)
print(c2)
